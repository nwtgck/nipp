{
  "name": "nipp",
  "version": "0.13.8-SNAPSHOT",
  "private": true,
  "description": "Mini Application Hosted on URL",
  "author": "Ryo Ota <nwtgck@nwtgck.org> (https://github.com/nwtgck)",
  "scripts": {
    "serve": "npm run copy-scripts && vue-cli-service serve",
    "build": "npm run copy-scripts && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "deploy": "npm run build && gh-pages -m '[AUTO] Upload pages' -d dist -a",
    "copy-babel": "copyfiles --flat node_modules/@babel/standalone/babel.min.js public/copied_js",
    "copy-lzma": "copyfiles --flat node_modules/lzma/src/lzma_worker-min.js public/copied_js",
    "copy-opal": "copyfiles opal-cdn/opal/current/*.min.js public",
    "copy-scripts": "npm run copy-opal && npm run copy-lzma && npm run copy-babel",
    "test": "npm run test:unit"
  },
  "dependencies": {
    "@babel/standalone": "^7.20.12",
    "core-js": "^3.6.4",
    "lzma": "^2.3.2",
    "monaco-editor": "^0.20.0",
    "pako": "^1.0.11",
    "purecss": "^3.0.0",
    "register-service-worker": "^1.6.2",
    "ua-device-detector": "^1.1.8",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-monaco": "^1.2.2",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^7.0.1",
    "@types/pako": "^1.0.1",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-pwa": "^4.5.19",
    "@vue/cli-plugin-typescript": "^4.5.19",
    "@vue/cli-plugin-unit-mocha": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/test-utils": "^1.0.0-beta.31",
    "chai": "^4.3.7",
    "copyfiles": "^2.2.0",
    "gh-pages": "^2.2.0",
    "monaco-editor-webpack-plugin": "1.9",
    "typescript": "^4.9.4",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5"
  },
  "bugs": {
    "url": "https://github.com/nwtgck/nipp/issues"
  },
  "homepage": "https://github.com/nwtgck/nipp#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwtgck/nipp.git"
  }
}
